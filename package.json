{
  "name": "domxenh.io.vn",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "convert:webp": "node scripts/convert-public-webp.mjs",
    "watch:webp": "chokidar \"public/**/*.{png,jpg,jpeg}\" -c \"node scripts/convert-public-webp.mjs\"",
    "predev": "npm run convert:webp && npx prisma generate && npx prisma db seed",
    "dev": "concurrently -n WEBP,NEXT -c yellow,cyan \"npm run watch:webp\" \"next dev\"",
    "build": "prisma generate && next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck && npm run build",
    "clean": "rimraf .next"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "dotenv": "^17.3.1",
    "framer-motion": "^12.34.3",
    "next": "16.1.6",
    "prisma": "^5.22.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.24",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.3",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
